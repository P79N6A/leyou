package com.xg.admin.dto.npc;

import com.xg.admin.pojo.Npc;

/**
 * npc
 * @author zhangyaping email:yapingzhang_beta@163.com
 *
 */
public class NpcDto {
	private int id;
	private String name;

	public NpcDto(int id, String name) {
		this.id = id;
		this.name = name;
	}

	public NpcDto(Npc n) {
		this.id = n.getNpcId();
		this.name = n.getName();
	}

	

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
