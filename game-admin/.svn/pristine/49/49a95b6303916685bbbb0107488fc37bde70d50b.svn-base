package com.game.admin.pojo;

import java.io.Serializable;

public class HeroModel  implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 4916993732511279434L;
	private int id;
	private String name;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public HeroModel(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	public HeroModel() {
		super();
	}
	
	
	
	
}
