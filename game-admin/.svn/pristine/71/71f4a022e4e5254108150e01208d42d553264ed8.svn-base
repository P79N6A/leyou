package com.game.admin.dto.chat;

public class ChatTypeData {
	private int type;
	private String typeName;
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public String getTypeName() {
		return typeName;
	}
	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}
	public ChatTypeData() {
	}
	
	public static ChatTypeData valueOf(int type, String typeName) {
		ChatTypeData data = new ChatTypeData();
		data.setType(type);
		data.setTypeName(typeName);
		return data;
	}
	
}
